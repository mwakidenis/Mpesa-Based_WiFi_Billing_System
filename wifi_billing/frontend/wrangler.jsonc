{
  "name": "wifi-billing",
  "main": "src/index.js",
  "compatibility_date": "2025-12-17",
  "build": {
    // Install dependencies, build, and export static files
    "command": "pnpm install --no-frozen-lockfile && pnpm build && pnpm export",
    // The folder containing the final output after build/export
    "directory": "out",
    // Remove cwd if frontend code is at repo root
    "cwd": "."
  }
}
